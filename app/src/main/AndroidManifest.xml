<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.gb.m_2090_3">


    <uses-permission android:name="android.permission.INTERNET"/>



    <application
        android:name="MyApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/SplashTheme"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/MyNewTheme">
        </activity>

        <activity
            android:name=".view.ux.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".view.ux.UXActivity"
            android:exported="true"
            android:label="@string/app_name">
        </activity>

        <activity
            android:name=".view.layouts.LayoutActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/MyNewTheme">
        </activity>


        <activity
            android:name=".view.animation.AnimationActivity"
            android:exported="true">
        </activity>

        <activity
            android:name=".view.recycler.RecyclerActivity"
            android:exported="true">

        </activity>

        <activity
            android:name=".view.navigation.ViewPagerActivity"
            android:exported="true">

        </activity>

        <activity
            android:name=".view.navigation.BottomBarActivity"
            android:exported="true">

        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>