<resources>

    <style name="Base.Theme.M_2090_3" parent="Theme.Material3.DynamicColors.DayNight">

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <item name="myColor">@color/my_color</item>
        <item name="myColor2">@color/my_color_test</item>

        <item name="appBarLayoutStyle">@style/MyBottomNavigationView</item>

        <item name="shapeAppearanceMediumComponent">@style/MyShapeAppearanceMediumComponent</item>
    </style>

    <style name="MyNewTheme" parent="Base.Theme.M_2090_3" />

    <style name="SplashTheme" parent="MyNewTheme" >
        <item name="android:windowBackground">@drawable/splash_background</item>
        <item name="android:statusBarColor">@color/white</item>
        <item name="android:navigationBarColor">@color/white</item>
    </style>

    <style name="MyNewTheme.MyNewNewTheme" />

    <style name="MyNewNewNewTheme" parent="MyNewTheme.MyNewNewTheme" />

    <style name="MyRedTheme" parent="MyNewNewNewTheme">
        <item name="myColor">#FF0101</item>
    </style>

    <style name="MyGreenTheme" parent="MyNewNewNewTheme">
        <item name="myColor">#1EF41E</item>
    </style>

    <style name="MyBlueTheme" parent="MyNewNewNewTheme">
        <item name="myColor">#0D96FF</item>
    </style>



</resources>